class A{
  constructor(){
    this.a = [1,2,3,4,5,6];
      this.addMore = this.addMore.bind(this);
        this.some = () => this.say();
  }

  addMore(num){
    this.a.push(num);
    console.log(this.a);
  }

  say(){
    console.log('say hi');
  }

}

let myA = new A();

myA.addMore(23)
myB = myA.some
myB();
myB();


