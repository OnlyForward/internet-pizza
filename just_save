var factorial = function f(n) {
  return n==1 ? 1 : n*f(n-1);
};

/**
 –í —Å—Ç–∞—Ä–æ–º —Å—Ç–∞–Ω–¥–∞—Ä—Ç–µ JavaScript –æ–±—ä–µ–∫—Ç arguments –Ω–µ —Ç–æ–ª—å–∫–æ —Ö—Ä–∞–Ω–∏–ª —Å–ø–∏—Å–æ–∫ –∞—Ä–≥—É–º–µ–Ω—Ç–æ–≤, 
 –Ω–æ –∏ —Å–æ–¥–µ—Ä–∂–∞–ª –≤ —Å–≤–æ–π—Å—Ç–≤–µ arguments.callee —Å—Å—ã–ª–∫—É –Ω–∞ —Ñ—É–Ω–∫—Ü–∏—é, 
 –∫–æ—Ç–æ—Ä–∞—è –≤—ã–ø–æ–ª–Ω—è–µ—Ç—Å—è –≤ –¥–∞–Ω–Ω—ã–π –º–æ–º–µ–Ω—Ç.
 */
// –ø–æ–¥–≤—ã–∑–æ–≤ —á–µ—Ä–µ–∑ arguments.callee
var factorial1 = function(n) {
  return n==1 ? 1 : n*arguments.callee(n-1);
};

console.log(factorial(5));
console.log(factorial1(5));


