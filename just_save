function f1() {
  console.log( arguments.callee.caller ); // null, –º–µ–Ω—è –≤—ã–∑–≤–∞–ª–∏ –∏–∑ –≥–ª–æ–±–∞–ª—å–Ω–æ–≥–æ –∫–æ–¥–∞
  f2();
}

function f2() {
  console.log( arguments.callee.caller ); // f1, —Ñ—É–Ω–∫—Ü–∏—è, –∏–∑ –∫–æ—Ç–æ—Ä–æ–π –º–µ–Ω—è –≤—ã–∑–≤–∞–ª–∏
  f3();
}

