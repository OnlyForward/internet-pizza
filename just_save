/**
 try..catch –ø–æ–¥—Ä–∞–∑—É–º–µ–≤–∞–µ—Ç, —á—Ç–æ –∫–æ–¥ —Å–∏–Ω—Ç–∞–∫—Å–∏—á–µ—Å–∫–∏ –≤–µ—Ä–µ–Ω

 –û—à–∏–±–∫—É, –∫–æ—Ç–æ—Ä–∞—è –ø—Ä–æ–∏–∑–æ–π–¥—ë—Ç –≤ –∫–æ–¥–µ, –∑–∞–ø–ª–∞–Ω–∏—Ä–æ–≤–∞–Ω–Ω–æ–º ¬´–Ω–∞ –±—É–¥—É—â–µ–µ¬ª, 
 –Ω–∞–ø—Ä–∏–º–µ—Ä –≤ setTimeout, try..catch –Ω–µ –ø–æ–π–º–∞–µ—Ç:
 */

/**
 name
–¢–∏–ø –æ—à–∏–±–∫–∏. –ù–∞–ø—Ä–∏–º–µ—Ä, –ø—Ä–∏ –æ–±—Ä–∞—â–µ–Ω–∏–∏ –∫ –Ω–µ—Å—É—â–µ—Å—Ç–≤—É—é—â–µ–π –ø–µ—Ä–µ–º–µ–Ω–Ω–æ–π: "ReferenceError".
message
–¢–µ–∫—Å—Ç–æ–≤–æ–µ —Å–æ–æ–±—â–µ–Ω–∏–µ –æ –¥–µ—Ç–∞–ª—è—Ö –æ—à–∏–±–∫–∏.
stack
–í–µ–∑–¥–µ, –∫—Ä–æ–º–µ IE8-, –µ—Å—Ç—å —Ç–∞–∫–∂–µ —Å–≤–æ–π—Å—Ç–≤–æ stack, –∫–æ—Ç–æ—Ä–æ–µ —Å–æ–¥–µ—Ä–∂–∏—Ç —Å—Ç—Ä–æ–∫—É —Å –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–µ–π 
–æ –ø–æ—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å–Ω–æ—Å—Ç–∏ –≤—ã–∑–æ–≤–æ–≤, –∫–æ—Ç–æ—Ä–∞—è –ø—Ä–∏–≤–µ–ª–∞ –∫ –æ—à–∏–±–∫–µ.
 */

/**
 –ë–ª–æ–∫ finally —Å—Ä–∞–±–∞—Ç—ã–≤–∞–µ—Ç –ø—Ä–∏ –ª—é–±–æ–º –≤—ã—Ö–æ–¥–µ –∏–∑ try..catch, –≤ —Ç–æ–º —á–∏—Å–ª–µ –∏ return.
 */

/**
 –í –±—Ä–∞—É–∑–µ—Ä–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç —Å–ø–µ—Ü–∏–∞–ª—å–Ω–æ–µ —Å–≤–æ–π—Å—Ç–≤–æ window.onerror, –µ—Å–ª–∏ –≤ –Ω–µ–≥–æ –∑–∞–ø–∏—Å–∞—Ç—å —Ñ—É–Ω–∫—Ü–∏—é, 
 —Ç–æ –æ–Ω–∞ –≤—ã–ø–æ–ª–Ω–∏—Ç—Å—è –∏ –ø–æ–ª—É—á–∏—Ç –≤ –∞—Ä–≥—É–º–µ–Ω—Ç–∞—Ö —Å–æ–æ–±—â–µ–Ω–∏–µ –æ—à–∏–±–∫–∏, —Ç–µ–∫—É—â–∏–π URL –∏ –Ω–æ–º–µ—Ä —Å—Ç—Ä–æ–∫–∏, 
 –æ—Ç–∫—É–¥–∞ ¬´–≤—ã–ø–∞–ª–∞¬ª –æ—à–∏–±–∫–∞.

–æ–±–µ—Ä–Ω—É—Ç –≤ script
  window.onerror = function(message, url, lineNumber) {
    alert("–ü–æ–π–º–∞–Ω–∞ –æ—à–∏–±–∫–∞, –≤—ã–ø–∞–≤—à–∞—è –≤ –≥–ª–æ–±–∞–ª—å–Ω—É—é –æ–±–ª–∞—Å—Ç—å!\n" +
      "–°–æ–æ–±—â–µ–Ω–∏–µ: " + message + "\n(" + url + ":" + lineNumber + ")");
  };

  function readData() {
    error(); // –æ–π, —á—Ç–æ-—Ç–æ –Ω–µ —Ç–∞–∫
  }

  readData();

 */

/*
–ú—ã —É–∂–µ —Ä–∞–±–æ—Ç–∞–ª–∏ –≤ –û–û–ü-—Å—Ç–∏–ª–µ, —Å–æ–∑–¥–∞–≤–∞—è –æ–±—ä–µ–∫—Ç—ã —Ç–∞–∫–æ–≥–æ –≤–∏–¥–∞:

 function User(name) {

  this.sayHi = function() {
    alert( "–ü—Ä–∏–≤–µ—Ç, —è " + name );
  };

}

var vasya = new User("–í–∞—Å—è"); // —Å–æ–∑–¥–∞–ª–∏ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è
vasya.sayHi(); // –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å —É–º–µ–µ—Ç –≥–æ–≤–æ—Ä–∏—Ç—å "–ü—Ä–∏–≤–µ—Ç"
 */

/**
 –ö–ª–∞—Å—Å–æ–º –≤ –æ–±—ä–µ–∫—Ç–Ω–æ-–æ—Ä–∏–µ–Ω—Ç–∏—Ä–æ–≤–∞–Ω–Ω–æ–π —Ä–∞–∑—Ä–∞–±–æ—Ç–∫–µ –Ω–∞–∑—ã–≤–∞—é—Ç —à–∞–±–ª–æ–Ω
  –ø—Ä–æ–≥—Ä–∞–º–º–Ω—ã–π –∫–æ–¥, –ø—Ä–µ–¥–Ω–∞–∑–Ω–∞—á–µ–Ω–Ω—ã–π –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è –æ–±—ä–µ–∫—Ç–æ–≤ –∏ –º–µ—Ç–æ–¥–æ–≤.

–í JavaScript –∫–ª–∞—Å—Å—ã –º–æ–∂–Ω–æ –æ—Ä–≥–∞–Ω–∏–∑–æ–≤–∞—Ç—å –ø–æ-—Ä–∞–∑–Ω–æ–º—É. –ì–æ–≤–æ—Ä—è—Ç, —á—Ç–æ –∫–ª–∞—Å—Å User 
–Ω–∞–ø–∏—Å–∞–Ω –≤ ¬´—Ñ—É–Ω–∫—Ü–∏–æ–Ω–∞–ª—å–Ω–æ–º¬ª —Å—Ç–∏–ª–µ. –î–∞–ª–µ–µ –º—ã —Ç–∞–∫–∂–µ —É–≤–∏–¥–∏–º ¬´–ø—Ä–æ—Ç–æ—Ç–∏–ø–Ω—ã–π¬ª —Å—Ç–∏–ª—å.
 */

/**
 function CoffeeMachine(power) {
  this.waterAmount = 0; // –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –≤–æ–¥—ã –≤ –∫–æ—Ñ–µ–≤–∞—Ä–∫–µ

  alert( '–°–æ–∑–¥–∞–Ω–∞ –∫–æ—Ñ–µ–≤–∞—Ä–∫–∞ –º–æ—â–Ω–æ—Å—Ç—å—é: ' + power + ' –≤–∞—Ç—Ç' );
}

// —Å–æ–∑–¥–∞—Ç—å –∫–æ—Ñ–µ–≤–∞—Ä–∫—É
